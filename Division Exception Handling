import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        try{
            int A=scan.nextInt();
            int B=scan.nextInt();
            int C=A/B;
            System.out.println(C);
        }
        catch(ArithmeticException e){
            System.out.println("Division by zero is not allowed");
        }
    }
}
