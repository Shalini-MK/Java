import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int a=scan.nextInt();
        int b=0;
        int c=a;
        while(c>0){
             b=b*10+c%10;
             c= c/10;    
        }
        if(a==b){
            System.out.print("Palindrome");
        }
        else{
            System.out.println("Not Palindrome");
        }
     
    }
}


