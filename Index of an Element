import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     
       Scanner scan=new Scanner(System.in);
       int X=scan.nextInt(); 
       int a[]=new int[X];   
       for(int i=0;i<X;i++){
           a[i]=scan.nextInt();
       }
       int index=-1;
       for(int i=0;i<X;i++){
       if(a[i]==X){
           index=i;
           break;
       }
       }
        System.out.print(index);
       
    }
}

