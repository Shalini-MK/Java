import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
       int a;

        do {
            a=scan.nextInt();
            if (a!= 0) {
                System.out.println(a);
            }
        } while (a!= 0);
        scan.close();
    }
}


    
