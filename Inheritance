import java.io.*;
import java.util.*;
class Parent{
    void displayParent(){
        System.out.println("Parent");
    }
}
class Child extends Parent{
    void displayChild(){
        System.out.println("Child");
    }
}
public class Solution {
    public static void main(String[] args) {
        Child c=new Child();
        c.displayParent();
        c.displayChild();
        
    }
}
