import java.io.*;
import java.util.*;
class BankAccount{
    String name;
    int initial;
    int deposit;
    BankAccount(String n,int i,int d){
        name=n;
        initial=i;
        deposit=d;
    }
    public double deposit(){
       
        double balance=initial+deposit;
        return balance;
    }
void display(){
    System.out.println("Final balance:"+deposit());
    
}
}

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        String a=scan.nextLine();
        int b=scan.nextInt();
        int c=scan.nextInt();

        BankAccount s1=new BankAccount(a,b,c);
        s1.display();
       
    }
}
