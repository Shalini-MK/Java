import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int n = scan.nextInt();
        int[] arr = new int[n];
        int largest=arr[0];

        for(int i=0;i<n;i++) {
            arr[i] = scan.nextInt();
        }

        for (int i =1;i<n;i++) {
            if (arr[i]>largest) {
                largest=arr[i];
            }
        }

        System.out.println(largest);
    }
}
