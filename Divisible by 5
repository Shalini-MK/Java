import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
       Scanner scan=new Scanner(System.in);
       int n=scan.nextInt();
       if(n%5==0){
           System.out.print("Yes");
       }
       else{
            System.out.print("No");
        }
    }
}
    
