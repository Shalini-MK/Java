import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        if(n%4==0){
            System.out.print("Leap Year");
            }
        else{
            System.out.println("Not Leap Year");
        }      
    }
}
