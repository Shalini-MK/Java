import java.io.*;
import java.util.*;

public class Solution {
    static long power(int a,int b){
        long c=1;
        for(int i=0;i<b;i++){
            c*=a;
        }
        return c;
    }

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int a=scan.nextInt();
        int b=scan.nextInt();
        System.out.print(power(a,b));
        
    }
}
