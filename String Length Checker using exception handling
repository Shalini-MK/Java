import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        try{
            int N=scan.nextInt();
            int[] arr=new int[N];
            for(int i = 0;i < N;i++){
                arr[i]=scan.nextInt();
            }
            int K=scan.nextInt();
            System.out.println(arr[K]);
        }
        catch(ArrayIndexOutOfBoundsException e){
            System.out.println("Invalid Index");
        }
    }
}
